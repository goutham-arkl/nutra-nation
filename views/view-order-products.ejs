<%-include('partials/header')-%> 

<section style="height: 80vh;">
    <div class="container ">
        <div class="row">
            
         
        </div>
        <div class="row mt-5">
            <table class="table" id="example">
                <thead >
                    <tr class="bg-dark text-white">
                        <th scope="col">No</th>
                        <th scope="col">Image</th>
                        <th scope="col">Name</th>
                       
                        <th scope="col">Quantity</th>
                        <th scope="col">Category</th>
                        <th scope="col">Price</th>
                        
                       
                    </tr>
                </thead>
                <tbody>
                   <%for(i=0;i<Orderedproducts.length;i++){%>
                    <tr>
                        <th scope="row"><%=i+1%></th>
                        <td>
                            <img style="width: 50px;height: 50px;" src="/images/products/<%=Orderedproducts[i].product._id%>.jpg" alt="image">
                        </td>
                        <td><%=Orderedproducts[i].product.name%></td>
                        
                        <td><%=Orderedproducts[i].quantity%></td>
                        <td><%=Orderedproducts[i].product.category%></td>
                        <td><%=Orderedproducts[i].product.price%></td>
                       
                       
                       
                        
                    </tr>
                    <%}%>
                </tbody>
            </table>
        </div>
  
    </div>
  </section>

<%-include('partials/footer')-%>