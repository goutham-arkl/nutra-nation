<%-include('partials/header')-%> 

<section>
    <div class="container">
        <div class="row">
            
         
        </div>
        <div class="row">
            <table class="table" id="example">
                <thead>
                    <tr>
                        <th scope="col">No</th>
                        <th scope="col">Name</th>
                        <th scope="col">Quantity</th>
                        <th scope="col">Category</th>
                        <th scope="col">Price</th>
                        
                       
                    </tr>
                </thead>
                <tbody>
                   <%for(i=0;i<Orderedproducts.length;i++){%>
                    <tr>
                        <th scope="row"><%=i+1%></th>
                        <td><%=Orderedproducts[i].product.name%></td>
                        <td><%=Orderedproducts[i].quantity%></td>
                        <td><%=Orderedproducts[i].product.category%></td>
                        <td><%=Orderedproducts[i].product.price%></td>
                       
                       
                       
                        
                    </tr>
                    <%}%>
                </tbody>
            </table>
        </div>
  
    </div>
  </section>

<%-include('partials/footer')-%>